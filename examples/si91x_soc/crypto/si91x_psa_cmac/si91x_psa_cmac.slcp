project_name: si91x_psa_cmac
label: SI91x - SoC PSA CMAC
description: |
  This example project demonstrates the PSA CMAC APIs in SI91x.
category: example|crypto
package: platform
quality: production
sdk:
  id: gecko_sdk
  version: 4.4.3
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.2.0
source:
  - path: main.c
  - path: psa_cmac_app.c
  - path: app.c
include:
  - path: .
    file_list:
      - path: psa_cmac_app.h
      - path: app.h
toolchain_settings: []
component:
  - id: sl_system
  - id: freertos
  - id: psa_crypto
  - id: psa_driver
  - id: psa_crypto_cmac
  - id: psa_crypto_trng
  - id: sl_si91x_psa_wrap
    from: wiseconnect3_sdk
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si917_memory_default_config
    from: wiseconnect3_sdk
  - id: wifi
    from: wiseconnect3_sdk
  - id: sl_si91x_internal_stack
    from: wiseconnect3_sdk
  - id: wifi_resources
    from: wiseconnect3_sdk
  - id: network_manager
    from: wiseconnect3_sdk
  - id: basic_network_config_manager
    from: wiseconnect3_sdk
  - id: sl_si91x_basic_buffers
    from: wiseconnect3_sdk

  - id: mbedtls_cmac
  - id: sl_si91x_psa_mac
    from: wiseconnect3_sdk
readme:
  - path: readme.md
other_file:
  - path: resources/readme/image508a.png
  - path: resources/readme/image508c.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
post_build:
  path: ../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
