project_name: si91x_test_aead
label: SI91x - SoC PSA AEAD Test
description: |
  PSA AEAD API Test in SI91x.
category: Test|crypto
package: platform
quality: internal
filter:
  - name: Wireless Technology
    value:
      - Wi-Fi
  - name: Project Difficulty
    value:
      - Beginner
readme:
  - path: readme.md
source:
  - path: main.c
  - path: app.c
  - path: psa_driver_aead_test.c
include:
  - path: .
    file_list:
      - path: app.h
      - path: psa_driver_aead_test.h
      - path: psa_driver_aead_test_data.h
sdk:
  id: simplicity_sdk
  version:  "2024.12.0"
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.4.0
component:
  - id: sl_system
  - id: freertos
  - id: freertos_heap_4
  - id: psa_crypto_ccm
  - id: psa_crypto_gcm
  - id: psa_crypto_chachapoly
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si91x_memory_default_config
    from: wiseconnect3_sdk
  - id: wifi
    from: wiseconnect3_sdk
  - id: sl_si91x_internal_stack
    from: wiseconnect3_sdk
  - id: wifi_resources
    from: wiseconnect3_sdk
  - id: network_manager
    from: wiseconnect3_sdk
  - id: basic_network_config_manager
    from: wiseconnect3_sdk
  - id: sl_si91x_basic_buffers
    from: wiseconnect3_sdk
  - id: unity_si91x
    from: wiseconnect3_sdk
other_file:
  - path: resources/readme/image508a.png
  - path: resources/readme/output.png
ui_hints:
  highlight:
    - path: readme.md
      focus: true
toolchain_settings:
  - option: gcc_compiler_option
    value: -Wall -Werror
post_build:
  path: ../../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
