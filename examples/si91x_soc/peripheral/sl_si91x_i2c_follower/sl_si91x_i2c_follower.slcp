project_name: sl_si91x_i2c_follower
label: SI91x - SL_I2C_Follower
description: |
  Demonstrates how to use the I2C interface on the SI91x SoC device.
category: example|peripheral
package: platform
quality: production
sdk:
  id: gecko_sdk
  version: 4.4.3
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.2.0
source:
  - path: main.c
  - path: app.c
  - path: app.h
  - path: i2c_follower_example.c
  - path: i2c_follower_example.h
toolchain_settings: []
component:
  - id: sl_system
  - id: status
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si917_memory_default_config
    from: wiseconnect3_sdk
  - id: sl_i2c_peripheral
    from: wiseconnect3_sdk
define:
  - name: SLI_SI91X_MCU_MOV_ROM_API_TO_FLASH
other_file:
  - path: resources/readme/image506d.png
  - path: resources/readme/image506e.png
  - path: resources/readme/setupdiagram.png
  - path: resources/readme/output.png
readme:
  - path: readme.md
ui_hints:
  highlight: 
    - path: readme.md
      focus: true
post_build:
  path: ../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
