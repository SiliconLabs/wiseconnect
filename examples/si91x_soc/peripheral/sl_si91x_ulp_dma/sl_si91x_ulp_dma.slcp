project_name: sl_si91x_ulp_dma
label: SI91x - SL_ULP_DMA
description: |
  This application demonstrates UDMA functionality.
category: example|peripheral
package: platform
quality: production
readme:
  - path: readme.md
source:
  - path: app.c
  - path: main.c
  - path: ulp_dma_example.c
  - path: ulp_dma_example.h
  - path: app.h
sdk:
  id: simplicity_sdk
  version:  "2024.12.2"
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.4.2
toolchain_settings:
  - option: gcc_compiler_option
    value: -Wall -Werror
component:
  - id: sl_system
  - id: status
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si91x_memory_default_config
    from: wiseconnect3_sdk
  - id: sl_dma
    from: wiseconnect3_sdk
  - id: sl_power_manager
    from: wiseconnect3_sdk
  - id: debug_ulp_component
    from: wiseconnect3_sdk
  - id: dma_ulp_component
    from: wiseconnect3_sdk 
  - id: sllib_wireless_shutdown
    from: wiseconnect3_sdk
template_contribution:
  - name: user_files_ps2
    value: ulp_dma_example.o
  - name: user_files_ps2
    value: app.o
  - name: user_files_ps2
    value: main.o
  - name: user_files_ps2
    value: sl_system_process_action.o
  - name: user_files_ps2
    value: sl_event_handler.o
other_file:
  - path: resources/readme/setupdiagram.png
  - path: resources/readme/ulp_dma_uc_screen.png
  - path: resources/readme/output_ulp_dma.png
define:
  - name: __STATIC_INLINE
    value: static inline
  - name: SLI_SI91X_MCU_ENABLE_RAM_BASED_EXECUTION
  - name: SL_SI91X_ULP_STATE_ENABLE
ui_hints:
  highlight:
    - path: readme.md
      focus: true
post_build:
  path: ../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
