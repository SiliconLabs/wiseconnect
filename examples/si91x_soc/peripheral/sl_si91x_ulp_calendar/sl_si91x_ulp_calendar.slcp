project_name: sl_si91x_ulp_calendar
label: SI91x - SL_ULP_CALENDAR
description: >
  Demonstrates the how to configure and use the ULP Calendar
  peripheral in SiLabs MCU chip.
category: example|peripheral
package: platform
quality: production
readme:
  - path: readme.md
source:
  - path: app.c
  - path: main.c
  - path: app.h
  - path: ulp_calendar_example.c
  - path: ulp_calendar_example.h
sdk:
  id: gecko_sdk
  version: 4.4.3
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.2.0
toolchain_settings: []
component:
  - id: sl_system
  - id: status
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si917_memory_default_config
    from: wiseconnect3_sdk
  - id: sl_calendar
    from: wiseconnect3_sdk
  - id: sllib_wireless_shutdown
    from: wiseconnect3_sdk
  - id: sl_ulp_timer
    from: wiseconnect3_sdk
  - from: wiseconnect3_sdk
    id: sl_power_manager
  - from: wiseconnect3_sdk
    id: debug_ulp_component
  - from: wiseconnect3_sdk
    id: calendar_ulp_component
other_file:
  - path: resources/readme/setupdiagram.png
  - path: resources/uc_screen/calendar_uc_screen.png
  - path: resources/readme/output_ulp_calendar.png
template_contribution:
- name: user_files_ps2
  value: ulp_calendar_example.o
- name: user_files_ps2 
  value: app.o
- name: user_files_ps2
  value: main.o
- name: user_files_ps2
  value: sl_system_process_action.o
- name: user_files_ps2
  value: sl_event_handler.o
ui_hints:
  highlight:
    - path: readme.md
      focus: true
post_build:
  path: ../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
