project_name: MEMLCD_Baremetal
label: SI91x - MEMLCD Baremetal
description: |
  Demonstrates memlcd can be used.
category: example|peripheral
package: platform
quality: production
sdk:
  id: gecko_sdk
  version: 4.4.3
sdk_extension:
  - id: wiseconnect3_sdk
    version: 3.2.0
source:
  - path: app.c
  - path: main.c
  - path: memlcd_app.c
include:
  - path: .
    file_list:
      - path: app.h
      - path: memlcd_app.h
toolchain_settings: []
component:
  - id: sl_system
  - id: sl_assert
  - id: sleeptimer
  - id: dmd_memlcd
  - id: glib
  - id: memlcd_917
    from: wiseconnect3_sdk
  - id: cmsis_spi
    from: wiseconnect3_sdk
  - id: sl_ulp_timer
    from: wiseconnect3_sdk
  - id: syscalls
    from: wiseconnect3_sdk
  - id: si917_memory_default_config
    from: wiseconnect3_sdk
  - instance: [timer0]
    from: wiseconnect3_sdk
    id: ulp_timers_instance
other_file:
  - path: resources/readme/setupdiagram.png
  - path: resources/readme/MEMLCD_OP_SCRN.png
define:
  - name: SPI_MULTI_SLAVE
  - name: SLI_SI91X_MCU_MOV_ROM_API_TO_FLASH
readme:
  - path: readme.md
ui_hints:
  highlight:
    - path: readme.md
      focus: true
post_build:
  path: ../../../../utilities/postbuild_profile/wiseconnect_soc.slpb
