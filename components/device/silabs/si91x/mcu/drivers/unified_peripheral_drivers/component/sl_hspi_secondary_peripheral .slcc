provides:
  - name: sl_hspi_secondary_peripheral
requires:
  - name: romdriver_gpdma
  - name: rsilib_gpdma
source:
  - path: src/sl_si91x_peripheral_hspi_secondary.c
include:
  - path: inc
    file_list:
      - path: sl_si91x_peripheral_hspi_secondary.h
