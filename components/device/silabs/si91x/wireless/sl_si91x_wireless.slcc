      802.11b: SL_WIFI_RATE_PROTOCOL_B_ONLY
      802.11g: SL_WIFI_RATE_PROTOCOL_G_ONLY
      802.11n: SL_WIFI_RATE_PROTOCOL_N_ONLY
      802.11ax: SL_WIFI_RATE_PROTOCOL_AX_ONLY
    data_rate:
