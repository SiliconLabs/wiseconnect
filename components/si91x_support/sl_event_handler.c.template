extern void sl_si91x_hardware_setup(void);
void sl_platform_init(void)
{
  SystemCoreClockUpdate();
  sli_si91x_platform_init();
#if defined SL_UART
  sl_uart_init(DEFAULT_UART, SL_UART_BAUDRATE, DEFAULT_UART_PIN_CONFIG);
#else
  RSI_Board_Init();
#endif
  sl_si91x_hardware_setup();
  osKernelInitialize();
}

